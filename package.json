{
  "name": "barasu",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "16.5.0"
  },
  "scripts": {
    "start:frontend": "TS_NODE_PROJECT=\"./frontend/config/webpack.tsconfig.json\" webpack serve --config ./frontend/config/webpack.config.ts --open --mode development",
    "build:frontend": "webpack --config ./frontend/config/webpack.config.ts --mode production",
    "start:server": "nodemon --exec ts-node server/app.ts",
    "build:server": "tsc --project server/tsconfig.json",
    "run:command": "ts-node -P server/tsconfig.json server/command/main.ts",
    "typeorm": "ts-node -P server/tsconfig.json node_modules/.bin/typeorm -f server/infra/database/typeorm/ormconfig.ts",
    "eslint:check": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "npx eslint --fix",
    "format:check": "prettier --check './**/*.{ts,tsx}'",
    "format:write": "prettier --write './**/*.{ts,tsx}'",
    "test": "jest",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/mysql2": "types/mysql2",
    "@types/node": "^16.3.3",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/uuid": "^8.3.1",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^6.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.2",
    "husky": "^7.0.2",
    "jest": "^27.3.0",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0",
    "yargs": "^17.1.1"
  },
  "dependencies": {
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.3.2",
    "mysql2": "^2.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.37"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "rootDir": "./server"
  }
}

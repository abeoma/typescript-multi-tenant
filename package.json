{
  "name": "barasu",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:server": "nodemon --exec ts-node server/app.ts",
    "start:frontend": "webpack serve --config ./frontend/config/webpack.config.ts --open --mode development",
    "build:frontend": "webpack --config ./frontend/config/webpack.config.ts --mode production",
    "eslint:check": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "npx eslint --fix",
    "format:check": "prettier --check './**/*.{ts,tsx}'",
    "format:write": "prettier --write './**/*.{ts,tsx}'",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.3.3",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^6.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}

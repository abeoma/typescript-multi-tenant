generator client {
  provider      = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("TENANT_DB_URL")
}

model user {
  id        String   @id
  email     String   @unique
  firstName String
  lastName  String
  isActive  Boolean
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
